{
    "project_info": {
        "name": "MonAndroid",
        "type": "android_control_system",
        "tech_stack": [
            "Go",
            "React",
            "TypeScript",
            "Electron"
        ]
    },
    "backend_identifiers": {
        "main_entry": "main.go",
        "http_port": "8080",
        "ws_port": "8081",
        "endpoints": {
            "health_check": "/health",
            "devices_list": "/api/devices",
            "devices_scan": "/api/devices/scan",
            "actions_execute": "/api/actions",
            "actions_batch": "/api/actions/batch",
            "websocket": "/ws"
        },
        "models": {
            "device": "Device",
            "device_group": "DeviceGroup",
            "action": "Action",
            "action_request": "ActionRequest",
            "macro": "Macro"
        },
        "services": {
            "device_manager": "DeviceManager",
            "action_dispatcher": "ActionDispatcher"
        }
    },
    "frontend_identifiers": {
        "main_entry": "src/main.tsx",
        "dev_port": "5173",
        "components": {
            "device_card": "DeviceCard",
            "device_grid": "DeviceGrid",
            "screen_view": "ScreenView",
            "control_panel": "ControlPanel",
            "action_bar": "ActionBar",
            "app": "App"
        },
        "services": {
            "api_client": "api",
            "websocket_hook": "useWebSocket",
            "device_service": "deviceService"
        },
        "store": {
            "app_store": "useAppStore"
        },
        "types": {
            "device": "Device",
            "action": "Action",
            "api_response": "APIResponse",
            "websocket_message": "WebSocketMessage"
        }
    },
    "action_types": {
        "tap": "tap",
        "swipe": "swipe",
        "input": "input",
        "key": "key",
        "open_app": "open_app",
        "install_apk": "install_apk",
        "push_file": "push_file"
    },
    "key_codes": {
        "back": 4,
        "home": 3,
        "menu": 82,
        "power": 26,
        "volume_up": 24,
        "volume_down": 25,
        "enter": 66
    },
    "ui_text": {
        "app_title": "Android Control",
        "app_subtitle": "Multi-Device Control System",
        "btn_scan_devices": "Scan Devices",
        "btn_clear_selection": "Clear Selection",
        "btn_batch_input": "Batch Input",
        "status_connected": "Connected",
        "status_disconnected": "Disconnected",
        "msg_no_devices": "No devices found",
        "msg_connect_devices": "Connect your Android devices via USB and click 'Scan Devices'"
    },
    "naming_conventions": {
        "go": "CamelCase for exported, camelCase for unexported",
        "typescript": "camelCase for variables/functions, PascalCase for components/types",
        "css": "kebab-case for classes",
        "files": "kebab-case for React components, snake_case for utilities"
    }
}